# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def minDepth(self, root):
        """
        :type root: TreeNode
        :rtype: int
        """
        if not root:
            return 0
        n=1
        level=[root]
        while level:
            temp=[]
            for item in level:
                if not item.left and not item.right:
                    return n
                else:
                    if item.left: temp.append(item.left)
                    if item.right: temp.append(item.right)
            level=temp
            n+=1
